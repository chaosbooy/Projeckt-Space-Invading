<Window x:Class="SpaceInvading.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpaceInvading"
        mc:Ignorable="d"
        Title="Smol" Height="450" Width="800" Background="Transparent">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="Resources/Images/Background/background_tree.png" />
        </Grid.Background>
        <Frame Name="MainContent" 
               Source="Resources/Pages/Lobby.xaml" 
               Visibility="Visible"
               NavigationUIVisibility="Hidden"
               Navigated="MainContent_Navigated"/>
        
    </Grid>
</Window>
